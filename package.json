{
  "name": "@status/abstract",
  "version": "1.0.0",
  "description": "Enforce abstract classes",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.mjs",
  "types": "dist/types.d.ts",
  "scripts": {
    "commit": "git-cz",
    "test": "cross-env TS_NODE_PROJECT='./test/tsconfig.json' mocha",
    "prebuild": "rimraf dist",
    "build": "parcel build",
    "watch": "parcel watch",
    "compile": "tsc",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "precoverage:test": "rimraf coverage",
    "coverage:test": "cross-env TS_NODE_PROJECT='./test/tsconfig.json' nyc mocha",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "prepublish": "npm run compile",
    "publish": "npx semantic-release",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli",
    "check": "npm-check -u"
  },
  "keywords": [
    "abstract",
    "framework",
    "enforce",
    "object",
    "module",
    "class",
    "prototype"
  ],
  "author": "jfrazx",
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "chai": "^4.3.7",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "mocha": "^10.6.0",
    "npm-check": "^6.0.1",
    "nyc": "^17.0.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.7",
    "semantic-release": "^24.0.0",
    "semantic-release-cli": "^5.4.6",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "mocha": {
    "recursive": true,
    "spec": "test/*.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/abstract"
  }
}
